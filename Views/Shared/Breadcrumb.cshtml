@{
}
@model IEnumerable<string>

@if(Model!=null){
    <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            @foreach(string id in Model)
            {
                <li class="breadcrumb-item"><a href="#">@id</a></li>
 
            }
        </ol>
    </nav>
    
}