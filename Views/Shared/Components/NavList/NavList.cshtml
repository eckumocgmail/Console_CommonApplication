@{
}
@model NavList
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@addTagHelper *, AngularApplication
<style>
    .nav-list-item{
        padding: 3px 10px;
    }
</style>

<div>
    @foreach(var kv in Model.Model)
    {
        <div class="nav-list-item">
            <a href="@(kv.Key)">@kv.Value</a>
        </div>    
    }    
</div>